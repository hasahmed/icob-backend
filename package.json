{
  "name": "begin-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/basic-auth": "^1.1.2",
    "@types/chance": "^1.0.1",
    "@types/express": "^4.16.0",
    "@types/knex": "^0.15.1",
    "@types/nodemailer": "^4.6.5",
    "@types/prompts": "^1.1.1",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "@types/uuid": "^3.4.4",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.2",
    "basic-auth": "^2.0.1",
    "bootstrap": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "chance": "^1.0.18",
    "concurrently": "^4.1.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "formik": "^1.3.2",
    "formsy-react": "^1.1.5",
    "formsy-react-components": "^1.0.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "knex": "^0.15.2",
    "nodemailer": "^4.7.0",
    "nodemon": "^1.18.6",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prompts": "git+https://github.com/hasahmed/prompts.git",
    "raf": "3.4.0",
    "random-name": "^0.1.2",
    "react": "^16.6.3",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-signup-login-component": "^1.2.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "sqlite": "^3.0.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "symlink-dir": "^2.0.2",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "0.6.2",
    "uuid": "^3.3.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "knex-script": "knex --knexfile src/backend/dist/backend/knexfile.js",
    "migrations": "npm run knex-script -- migrate:latest",
    "nodemon": "nodemon",
    "jest": "jest",
    "ln": "symlink-dir",
    "dist-clear": "rmtrash src/backend/dist",
    "link-common": "symlink-dir src/backend/dist/common node_modules/common",
    "init-config": "node src/backend/dist/backend/bin/init-config.js",
    "frontend:start": "node src/frontend/scripts/start.js",
    "frontend:build": "node src/frontend/scripts/build.js",
    "frontend:test": "node src/frontend/scripts/test.js --env=jsdom",
    "frontend:watch": "tsc -p src/frontend/tsconfig.json",
    "backend:build": "tsc -p src/backend",
    "backend:build:watch": "npm run backend:build -- --watch",
    "backend:test": "jest --config=src/backend/jest.conf.json",
    "backend:serve": "nodemon --config src/backend/nodemon.json src/backend/dist/backend/Server.js",
    "backend:watch": "concurrently --kill-others 'npm:backend:build:watch' 'npm:backend:serve'",
    "backend:rebuild:watch": "npm run dist-clear && npm run backend:build && npm run backend:watch",
    "postinstall": "backend:build && frontend:build && link-common"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.10",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "react-test-renderer": "^16.6.3",
    "redux-devtools-extension": "^2.13.7",
    "typescript": "^3.2.2"
  },
  "jest": {
    "rootDir": "src/frontend/",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/../../node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "src/frontend/tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "proxy": "http://localhost:3030/"
}
